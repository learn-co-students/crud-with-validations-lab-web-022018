<%= form_for @song do |f| %>
  <%=label_tag :title %>
  <%= f.text_field :title %><br>

  <%=label_tag :released %>
  <%= f.check_box :released %><br>

  <%=label_tag :release_year %>
  <%= f.text_field :release_year %><br>

  <%=label_tag :artist_name %>
  <%= f.text_field :artist_name %><br>

  <%=label_tag :genre %>
  <%= f.text_field :genre %><br>
  <%= f.submit %>
<% end %>
